<h1>New feature_status</h1>

<% form_for(@feature_status) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :displayName %><br />
    <%= f.text_area :displayName %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', feature_statuses_path %>